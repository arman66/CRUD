<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title><%= title %></title>
    <!-- materialize LINKS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/css/materialize.min.css">
    <link rel="stylesheet" href="/stylesheets/style.css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  </head>
  <body>

    <!-- Dropdown Structure -->
    <ul id="dropdown1" class="dropdown-content">



      <li><a href="/login">Log In</a></li>
      <li><a href="/signup">Sign Up</a></li>
      <li class="divider"></li>
      <li><a href="/matches">Matches</a></li>
    </ul>

    <nav>

      <div class="nav-wrapper teal lighten-2">
        <a href="/" class="brand-logo">Let's Keep Score <i class="large material-icons">compare</i> </a>
        <ul class="right hide-on-med-and-down">

          <% if(currentUser){%>
            <li><a href="/logout">Log Out</a></li>
            <li><a href="/matches">Your Matches</a></li>
            <li><a href="/matches/new">Create a Match</a></li>
            <li><a href="/match/stats">Stats</a></li>
          <%} else {%>
            <li><a href="/login">Log In</a></li>
            <li><a href="/signup">Sign Up</a></li>
          <%}%>

          <!-- Dropdown Trigger -->
          <li>
            <a class="dropdown-button" href="#!" data-activates="dropdown1"><i class="material-icons right">dehaze</i></a>
          </li>
        </ul>
      </div>
    </nav>


